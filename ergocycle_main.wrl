joints [
    USE WAIST,
    USE CRANK_GEAR,
    USE PEDALS_AXIS_R,
    USE PEDALS_AXIS_L,
    USE SADDLE_JOINT,
    USE SUPP_HANDLEBAR_JOINT,
    USE HANDLEBAR_JOINT
